{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "build": "tsc ",
    "start": "ts-node -r tsconfig-paths/register ./build/app.js",
    "dev": "nodemon",
    "lint": "eslint . --ext .ts",
    "db:restart": "pnpm m:delete && pnpm m:run && pnpm s:run",
    "lint.fix": "eslint . --ext .ts --fix",
    "m:generate": "sequelize-cli migration:generate --name",
    "m:run": "sequelize-cli db:migrate",
    "m:undo": "sequelize-cli db:migrate:undo",
    "m:delete": "sequelize-cli db:migrate:undo:all",
    "s:generate": "sequelize-cli seed:generate --name",
    "s:run": "sequelize-cli db:seed:all",
    "s:delete": "sequelize-cli db:seed:undo:all",
    "test": "jest --forceExit --detectOpenHandles --verbose"
  },
  "devDependencies": {
    "@ngneat/falso": "^7.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "20.3.2",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "sequelize-do-migrations": "^1.1.3",
    "sequelize-typescript": "^2.1.5",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.21.4"
  },
  "keywords": [],
  "author": "Moises Tarifa",
  "license": "ISC"
}
